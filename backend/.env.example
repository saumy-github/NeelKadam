# Example environment variables - copy to .env and fill in your values
# Database configuration
DB_USER=postgres
DB_HOST=localhost
DB_NAME=sih_db
DB_PASSWORD=your_password_here
        (root)
DB_PORT=5432

# Server configuration
PORT=3000

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
        (blue_carbon_registry_secret_key_2025)

# Blockchain Configuration
# Replace with your actual Infura or Alchemy project URL
PROVIDER_URL=https://sepolia.infura.io/v3/your-infura-project-id
        (https://sepolia.infura.io/v3/f4602c978cde41b681df04ea42c46513)
# Replace with the private key of the wallet that has NCCR admin rights
ADMIN_PRIVATE_KEY=your_admin_wallet_private_key